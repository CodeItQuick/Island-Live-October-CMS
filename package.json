    "docs:build": "vuepress build docs"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "laravel-mix": "^4.0.15"
  },
  "dependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/material": "^8.0.0",
    "es6-promise": "^4.2.6",
    "vue": "^2.6.10",
    "vuex": "^3.1.1"
  }
